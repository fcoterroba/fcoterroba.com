---
import '../styles/global.css';

export interface Props {
  title: string;
  description: string;
  lang?: string;
  image?: string;
  type?: string;
  canonical?: string;
}

const {
  title,
  description,
  lang = 'en',
  image = '/og-image.jpg',
  type = 'website',
  canonical = Astro.url.href,
} = Astro.props;

const siteName = 'fcoterroba';
