    <script>
      import { gsap } from 'gsap';
      import { ScrollTrigger } from 'gsap/ScrollTrigger';
      
      gsap.registerPlugin(ScrollTrigger);
      
      document.addEventListener('DOMContentLoaded', () => {
        initializePreferences();
        
        gsap.fromTo('main', 
          { opacity: 0, y: 20 },
          { opacity: 1, y: 0, duration: 0.8, ease: 'power2.out' }
        );
        
        gsap.utils.toArray('[data-animate]').forEach((element: any) => {
          const delay = element.dataset.delay || 0;
          const direction = element.dataset.direction || 'up';
          
          let fromProps: any = { opacity: 0 };
          if (direction === 'up') fromProps.y = 30;
          if (direction === 'left') fromProps.x = -30;
          if (direction === 'right') fromProps.x = 30;
          
          gsap.fromTo(element, 
            fromProps,
            {
              opacity: 1,
              x: 0,
              y: 0,
              duration: 0.8,
              delay: parseFloat(delay),
              ease: 'power2.out',
              scrollTrigger: {
                trigger: element,
                start: 'top 80%',
                toggleActions: 'play none none reverse'
              }
            }
          );
        });
      });

      function initializePreferences() {
        const browserLang = navigator.language.startsWith('es') ? 'es' : 'en';
        
        const prefersDark = window.matchMedia('(prefers-color-scheme: dark)').matches;
        
        const storedLang = localStorage.getItem('fcoterroba-lang') || browserLang;
        const storedTheme = localStorage.getItem('fcoterroba-theme') || (prefersDark ? 'dark' : 'light');
        
        applyLanguage(storedLang);
        applyTheme(storedTheme);
        setupPreferenceListeners();
      }

      function applyLanguage(lang) {
        document.documentElement.lang = lang;
        
        document.querySelectorAll('[data-es][data-en]').forEach(element => {
          const text = element.dataset[lang];
          if (text) {
            if (text.includes('<span') || text.includes('<br')) {
              element.innerHTML = text;
            } else {
              element.textContent = text;
            }
          }
        });
        
        document.querySelectorAll('.lang-btn').forEach(btn => {
          btn.classList.toggle('active', btn.dataset.lang === lang);
        });
        
        localStorage.setItem('fcoterroba-lang', lang);
      }

      function applyTheme(theme) {
        document.documentElement.setAttribute('data-theme', theme);
        
        document.querySelectorAll('.theme-btn').forEach(btn => {
          btn.classList.toggle('active', btn.dataset.theme === theme);
        });
        
        localStorage.setItem('fcoterroba-theme', theme);
      }

      function setupPreferenceListeners() {
        document.querySelectorAll('.lang-btn').forEach(btn => {
          btn.addEventListener('click', () => {
            const lang = btn.dataset.lang;
            applyLanguage(lang);
          });
        });
        
        document.querySelectorAll('.theme-btn').forEach(btn => {
          btn.addEventListener('click', () => {
            const theme = btn.dataset.theme;
            applyTheme(theme);
          });
        });
        
        window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', (e) => {
          const storedTheme = localStorage.getItem('fcoterroba-theme');
          if (!storedTheme) {
            applyTheme(e.matches ? 'dark' : 'light');
          }
        });
      }
    </script>
  </body>
</html>
